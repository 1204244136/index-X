    <dc:title id="id-1">[S4_01]某暗部的少女共栖 1X</dc:title>
    <dc:creator id="id-2">镰池和马</dc:creator>
    <dc:identifier>calibre:200</dc:identifier>
    <dc:identifier>uuid:5d4d171a-8fb5-41ec-b1af-4032e230f7d3</dc:identifier>
