    <dc:title id="id-1">新约 某魔法的禁书目录 18X</dc:title>
    <dc:creator id="id-2">镰池和马</dc:creator>
    <dc:identifier>calibre:874</dc:identifier>
    <dc:identifier id="BookId">uuid:79ef1b6e-4e56-43f8-8ccf-3edb7075d5ed</dc:identifier>
