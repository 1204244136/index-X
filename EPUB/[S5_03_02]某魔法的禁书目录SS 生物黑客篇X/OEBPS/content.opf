    <dc:title id="id">某魔法的禁书目录SS 生物黑客篇X</dc:title>
    <dc:creator id="id-1">镰池和马</dc:creator>
    <dc:identifier>calibre:905</dc:identifier>
    <dc:identifier id="BookId">uuid:5190b86c-5de8-46b3-8a0e-57aebac1a833</dc:identifier>
